{
  "name": "ts-transform-default-export",
  "version": "0.0.0",
  "license": "ISC",
  "description": "`export default foo` â†’ `module.exports = foo`",
  "author": "Alex Schneider <me@schneider.ax>",
  "repository": "https://github.com/axtgr/ts-transform-default-export",
  "keywords": [
    "typescript",
    "transformer",
    "module.exports",
    "exports"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint {src,test}/*.ts",
    "test": "mocha --require ts-node/register/transpile-only --timeout 0 test/[!_]*.ts",
    "prepack": "npm run build && npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "eslint --cache --fix"
    ],
    "*.md": "prettier --write"
  },
  "devDependencies": {
    "@types/mocha": "^8.0.1",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-problems": "^4.0.0",
    "eslint-plugin-prefer-let": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^8.1.1",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "typescript": ">=3.0.0"
  }
}
